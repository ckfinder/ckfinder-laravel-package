{
    "name": "lawrence-belo/ckfinder-laravel-package",
    "description": "CKFinder 3 package for Laravel",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=5.6.0",
        "laravel/framework": "^9.2",
        "pimple/pimple": "~3.0",
        "league/flysystem": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "spatie/flysystem-dropbox": "^2.0.6"
    },
    "autoload": {
        "psr-4": {
            "CKSource\\CKFinderBridge\\": "src/",
            "CKSource\\CKFinder\\": "_connector/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CKSource\\CKFinderBridge\\CKFinderServiceProvider"
            ]
        }
    }
}
